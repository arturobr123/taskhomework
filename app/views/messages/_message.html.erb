<div class='card z-depth-2'>
  <div class='card-content'>
    <span class='card-title'>
    </span>

    <h5 class="inline">

      <%- if message.admin_id %>
        <%= "Trabajador"  %>
      <% end %>

      <%- if message.user_id %>
        <%= "Estudiante"  %>
      <% end %>
      
    </h5>

    <small class="inline"> <%= message.timestamp %> </small>

    <p class='top15'>
      <%= message.body %>
    </p>
  </div>
</div>