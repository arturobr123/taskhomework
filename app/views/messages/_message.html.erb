<div class='card z-depth-2'>
  <div class='card-content'>
    <span class='card-title'>
    </span>

    <div class="row">
      <p class="alignleft lighter">
        <%- if message.admin_id %>
          <%= "Tasker"  %>
        <% end %>

        <%- if message.user_id %>
          <%= "Estudiante"  %>
        <% end %>
      </p>

      <p class="alignright fecha_pequena"> <%= message.timestamp %> </p>
    </div>


    <div class="">
      <%= message.body %>
    </div>



  </div>
</div>
